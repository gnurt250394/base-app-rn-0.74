{
  "name": "app",
  "version": "0.0.1",
  "main": "index.js",
  "private": true,
  "scripts": {
    "postinstall": "node ./scripts/setup.js",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "lint": "eslint  index.js ./scripts/*.js  ./src/** --ext .js,.ts,.tsx",
    "start": "npx react-native start --reset-cache",
    "format": "prettier --write **/*.{ts,tsx,js,json,md}",
    "splash": "node ./scripts/splash.js splash/splash.png FFFFFF 150 main BootSplash",
    "type:check": "yarn tsc --noEmit --skipLibCheck",
    "app-icon": "npx rn-ml appicon -s appicon/appicon.png",
    "app-icon:dev": "npx rn-ml appicon -s appicon/appicon-dev.png -f dev -icn AppIcon-Dev",
    "ios:notification:dev": "node ./scripts/ios.js push-notification env/dev.json ",
    "android:report": "node scripts/android.js report",
    "android:hash": "node scripts/android.js hash",
    "android:gen-key": "node scripts/android.js keystore",
    "ios:dev": "node scripts/ios.js run env/dev.json",
    "android:dev": "node scripts/android.js run env/dev.json Debug",
    "android:release": "node scripts/android.js run env/dev.json Release"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.1",
    "@gorhom/portal": "^1.0.14",
    "@react-native-community/netinfo": "11.3.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.2.6",
    "@shopify/flash-list": "1.6.4",
    "@shopify/react-native-skia": "1.2.3",
    "axios": "^1.7.2",
    "expo": "^51.0.0",
    "expo-constants": "~16.0.2",
    "expo-font": "^12.0.9",
    "expo-image": "^1.12.13",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.18",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.21",
    "i18next": "^23.12.2",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-fast-compare": "^3.2.2",
    "react-freeze": "^1.0.4",
    "react-i18next": "^15.0.0",
    "react-native": "0.74.3",
    "react-native-animateable-text": "^0.12.1",
    "react-native-bootsplash": "^6.0.0",
    "react-native-gesture-handler": "~2.16.1",
