    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-expo": "^11.0.12",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "patch-package": "^8.0.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
